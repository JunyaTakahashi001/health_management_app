<p id="notice"><%= notice %></p>

<h1>Admins Healths</h1>

 <div class="ml-auto">
  <%= link_to 'ログアウト', destroy_admin_session_path, method: :delete, class: 'btn btn-outline-dark' %>
 </div>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins_healths.each do |admins_health| %>
      <tr>
        <td><%= link_to 'Show', [:admins, admins_health] %></td>
        <td><%= link_to 'Edit', edit_admins_health_path(admins_health) %></td>
        <td><%= link_to 'Destroy', [:admins, admins_health], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admins Health', new_admins_health_path %>
